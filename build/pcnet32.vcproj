<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="pcnet32"
	SccProjectName="pcnet32"
	SccAuxPath=""
	SccLocalPath=".."
	SccProvider="MSSCCI:Jalindi Igloo">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="SanOS|Win32"
			OutputDirectory=".\..\bin"
			IntermediateDirectory=".\..\obj\pcnet32"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="2"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="1"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="TRUE"
				AdditionalIncludeDirectories="..\src\include"
				PreprocessorDefinitions="PCNET32_LIB;KERNEL"
				IgnoreStandardIncludePath="TRUE"
				StringPooling="TRUE"
				ExceptionHandling="FALSE"
				RuntimeLibrary="0"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="os/krnl.h"
				PrecompiledHeaderFile=".\..\obj\pcnet32/pcnet32.pch"
				AssemblerListingLocation=".\..\obj\pcnet32/"
				ObjectFile=".\..\obj\pcnet32/"
				ProgramDataBaseFileName=".\..\obj\pcnet32/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				CompileAs="0"
				UndefineAllPreprocessorDefinitions="FALSE"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				IgnoreImportLibrary="TRUE"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="krnl.lib $(NOINHERIT)"
				OutputFile="..\bin/pcnet32.sys"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\lib"
				IgnoreAllDefaultLibraries="TRUE"
				ProgramDatabaseFile=".\..\bin/pcnet32.pdb"
				GenerateMapFile="TRUE"
				MapFileName=".\..\obj\pcnet32/pcnet32.map"
				EntryPointSymbol="start"
				BaseAddress=""
				ImportLibrary=".\..\lib/pcnet32.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\bin/pcnet32.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1030"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="SanOSDebug|Win32"
			OutputDirectory=".\..\dbg\bin"
			IntermediateDirectory=".\..\dbg\obj\pcnet32"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="..\src\include"
				PreprocessorDefinitions="PCNET32_LIB;KERNEL;DEBUG"
				IgnoreStandardIncludePath="TRUE"
				ExceptionHandling="FALSE"
				RuntimeLibrary="0"
				BufferSecurityCheck="FALSE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="os/krnl.h"
				PrecompiledHeaderFile=".\..\dbg\obj\pcnet32/pcnet32.pch"
				AssemblerListingLocation=".\..\dbg\obj\pcnet32/"
				ObjectFile=".\..\dbg\obj\pcnet32/"
				ProgramDataBaseFileName=".\..\dbg\obj\pcnet32/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"
				UndefineAllPreprocessorDefinitions="FALSE"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				IgnoreImportLibrary="TRUE"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="krnl.lib $(NOINHERIT)"
				OutputFile="..\dbg\bin\pcnet32.sys"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\dbg\lib"
				IgnoreAllDefaultLibraries="TRUE"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="..\dbg\symbols\pcnet32.pdb"
				GenerateMapFile="TRUE"
				MapFileName=".\..\dbg\obj\pcnet32/pcnet32.map"
				EntryPointSymbol="start"
				BaseAddress=""
				ImportLibrary="..\dbg\lib/pcnet32.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\dbg\bin/pcnet32.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1030"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="lib"
			Filter="">
			<File
				RelativePath="..\src\lib\ctype.c">
			</File>
			<File
				RelativePath="..\src\sys\krnl\iop.c">
			</File>
			<File
				RelativePath="..\src\lib\string.c">
			</File>
		</Filter>
		<File
			RelativePath="..\src\sys\dev\pcnet32.c">
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>

<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="osldr"
	SccProjectName="osldr"
	SccAuxPath=""
	SccLocalPath=".."
	SccProvider="MSSCCI:Jalindi Igloo">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="SanOS|Win32"
			OutputDirectory=".\..\bin"
			IntermediateDirectory=".\..\obj\osldr"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="2"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="1"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="TRUE"
				OptimizeForProcessor="0"
				AdditionalIncludeDirectories="..\src\include"
				PreprocessorDefinitions="KERNEL;OSLDR"
				IgnoreStandardIncludePath="TRUE"
				StringPooling="TRUE"
				ExceptionHandling="FALSE"
				RuntimeLibrary="0"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="krnl.h"
				PrecompiledHeaderFile=".\..\obj\osldr/osldr.pch"
				AssemblerListingLocation=".\..\obj\osldr/"
				ObjectFile=".\..\obj\osldr/"
				ProgramDataBaseFileName=".\..\obj\osldr/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				CompileAs="0"
				UndefineAllPreprocessorDefinitions="FALSE"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED"
				AdditionalDependencies="$(NOINHERIT)"
				OutputFile=".\..\bin/osldr.dll"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				IgnoreAllDefaultLibraries="TRUE"
				ProgramDatabaseFile=".\..\bin/osldr.pdb"
				GenerateMapFile="TRUE"
				MapFileName=".\..\obj\osldr/osldr.map"
				OptimizeForWindows98="2"
				EntryPointSymbol="start"
				BaseAddress="0x00010000"
				ImportLibrary=".\..\bin/osldr.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\bin/osldr.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1030"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="SanOSDebug|Win32"
			OutputDirectory=".\..\dbg\bin"
			IntermediateDirectory=".\..\dbg\obj\osldr"
			ConfigurationType="2"
			UseOfMFC="0"

				BufferSecurityCheck="FALSE"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="krnl.h"
				PrecompiledHeaderFile=".\..\dbg\obj\osldr/osldr.pch"
				AssemblerListingLocation=".\..\dbg\obj\osldr/"
